# Example environment variables for the Chaos Contained project
# Copy this file to .env and fill in the values

# Django Backend
DJANGO_SECRET_KEY=your-secret-key
DJANGO_DEBUG=True
DATABASE_URL=postgres://user:pass@localhost:5432/chaos_contained
ALLOWED_HOSTS=localhost,127.0.0.1

# FastAPI Service
FASTAPI_SECRET=your-fastapi-secret
DJANGO_API_URL=http://localhost:8000
WHISPER_API_KEY=your-whisper-api-key
OPENAI_API_KEY=your-openai-api-key

# Redis & Celery
REDIS_URL=redis://localhost:6379/0
CELERY_BROKER_URL=redis://localhost:6379/1

# Firebase
FIREBASE_SERVER_KEY=your-firebase-server-key
FIREBASE_ADMIN_CREDENTIALS=path/to/firebase-admin.json

# Memory Encryption
MEMORY_AES_KEY=your-32-byte-aes-key

# OAuth Providers
SPOTIFY_CLIENT_ID=your-spotify-client-id
SPOTIFY_CLIENT_SECRET=your-spotify-client-secret
YOUTUBE_CLIENT_ID=your-youtube-client-id
YOUTUBE_CLIENT_SECRET=your-youtube-client-secret
APPLE_CLIENT_ID=your-apple-client-id
APPLE_CLIENT_SECRET=your-apple-client-secret